<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SABlues</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/Bootswatch.css" />
    @*Extra layout CSS:*@
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
  <div class="container-fluid">
    <a class="navbar-brand" asp-area="" asp-page="/Index">SABlues</a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor03" aria-controls="navbarColor03" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarColor03">
      <ul class="navbar-nav me-auto">
        <li class="nav-item">
          <a class="nav-link active" asp-area="" asp-page="/Index">Home
            <span class="visually-hidden">(current)</span>
          </a>
        </li>
            <li class="nav-item">
                <a class="nav-link text-dark" asp-area="" asp-page="/AboutUs/AboutUs">About Us</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-dark" asp-area="" asp-page="/OurBedlingtons/Index">Our Bedlingtons</a>
            </li>
        <li class="nav-item dropdown">
          <a class="nav-link text-dark dropdown-toggle" data-bs-toggle="dropdown" asp-area="" asp-page="/Pupps/Index" role="button" aria-haspopup="true" aria-expanded="false">Previous Litters</a>
          <div class="dropdown-menu">
            <a class="dropdown-item" asp-area="" asp-page="/Pupps/Index">View All Litters</a>
            <a class="dropdown-item" asp-area="" asp-page="/Pupps/GLitter">G-Litter</a>
            <a class="dropdown-item" asp-area="" asp-page="/Pupps/Clitter">C-Litter</a>
            <a class="dropdown-item" asp-area="" asp-page="/Pupps/ABBALitter">ABBA-Litter</a>
            <a class="dropdown-item" asp-area="" asp-page="/Pupps/Dlitter">D-Litter</a>
            <a class="dropdown-item" asp-area="" asp-page="/Pupps/Slitter">S-Litter</a>
            <a class="dropdown-item" asp-area="" asp-page="/Pupps/Blitter">B-Litter</a>
          </div>
        </li>
    @*  <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-page="/Pupps/Index">Puppies</a>
        </li>*@
      </ul>
      <form class="d-flex">
        <input class="form-control me-sm-2" type="text" placeholder="Search">
        <button class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
      </form>
    </div>
  </div>
</nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class=" footer text-muted">
        <div class="container">
            &copy; 2022 - SABlues
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="https://kit.fontawesome.com/c84f312e1a.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
